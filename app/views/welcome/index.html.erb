<div class="p-70">
  <h1>Welcome#index</h1>
  <p>Find me in app/views/welcome/index.html.erb</p>
  <% if user_signed_in? %>
      <div> <%= current_user.name %> </div>
      <div> <%= current_user.email %> </div>
      <div> <%= image_tag current_user.image %> </div>
      <% if current_user.type == "Admin" %>
         <%= render(:partial => "devise/create_plan", :locals => { :user => current_user})%>
      <% end %>
       <% if current_user.type == "Buyer" %>
         <%= render(:partial => "Buyer/create_subscription", :locals => { :user => current_user})%>
      <% end %>
  <% end %>


  <%= link_to "Logout", destroy_user_session_path, :method => :delete %>
</div>
